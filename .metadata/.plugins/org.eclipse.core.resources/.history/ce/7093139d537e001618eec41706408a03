
public class Song {
	
	private String title,
				   artist;
	
	private int    rating,
				   bpm;
	
	//-----------------------------------------//
	//constructor
	public Song(String title, String artist, int rating, int bpm){
		this.title=title;
		this.artist=artist;
		this.rating=rating;
		this.bpm=bpm;
	}
	//-----------------------------------------//
	//toString
	@Override
	public String toString(){
		return this.title;
	}
	//-----------------------------------------//

}
